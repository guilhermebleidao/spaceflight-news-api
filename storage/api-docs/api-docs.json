                                    },
                                    "updatedAt": {
                                        "type": "string",
                                        "example": "2022-05-16T22:43:44.148Z"
                                    },
                                    "featured": {
                                        "type": "boolean",
                                        "example": true
                                    }
                                },
                                "type": "object"
                            }
